<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="374.0" prefWidth="551.0" spacing="10" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ing.soft.quemadiariaproject.Controller.CodeVerificationController">

    <Label text="Ingrese el código de verificación" />
     <TextField fx:id="codigoField" />
     <Button fx:id="Verificar" onAction="#confirmarCodigo" style="-fx-background-color: #19A49C;" text="Verificar" textFill="WHITE" />

    <HBox spacing="10" />
   <Button fx:id="Iniciarsesion" onAction="#goToWelcome" style="-fx-background-color: #19A49C;" text="Iniciar Sesion" textFill="WHITE" />
    

</VBox>
